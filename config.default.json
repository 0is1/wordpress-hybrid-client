{
    "api": {
        "baseUrl": "http://yourDomain.com/wp-json",
        "timeout": 10000,
        "maxAttempt": 3
    },
    "debugEnabled": false,
    "title": "Your Blog title",
    "ionicConfig": {
        "views": {
            "transition": null,
            "maxCache": 10,
            "forwardCache": false
        },
        "scrolling": {
            "jsScrolling": false
        },
        "backButton": {
            "icon": null,
            "previousTitleText": null
        },
        "form": {
            "checkbox": null,
            "toggle": null
        },
        "spinner": {
            "icon": null
        },
        "tabs": {
            "style": null,
            "position": null
        },
        "templates": {
            "maxPrefetch": null
        },
        "navBar": {
            "alignTitle": null,
            "positionPrimaryButtons": null,
            "positionSecondaryButtons": null
        }
    },
    "menu": {
        "defaultState": {
            "state": "public.posts",
            "params": {}
        },
        "exposeAsideWhen": "(min-width:900px)",
        "list": [{
            "type": "internal",
            "trans": "menu.posts",
            "route": "public.posts",
            "icon": "icon ion-ios-paper"
        }, {
            "type": "internal",
            "trans": "menu.authors",
            "route": "public.authors",
            "icon": "icon ion-ios-people"
        }, {
            "type": "folder",
            "trans": "menu.pages",
            "icon": "icon ion-folder",
            "list": [{
                "type": "internal",
                "trans": "Page id:1",
                "route": "public.pages.id({ id: 1 })",
                "icon": "icon ion-android-document"
            }, {
                "type": "internal",
                "trans": "All",
                "route": "public.pages",
                "icon": "icon ion-android-document"
            }]
        }, {
            "type": "folder",
            "trans": "menu.categories",
            "icon": "icon ion-folder",
            "list": [{
                "type": "internal",
                "trans": "Uncategorized",
                "route": "public.taxonomies.slug({ term: 'category', slug: 'uncategorized' })",
                "icon": "icon ion-iphone"
            }, {
                "type": "internal",
                "trans": "All",
                "route": "public.taxonomies({ term: 'category' })",
                "icon": "icon ion-folder"
            }]
        }, {
            "type": "folder",
            "trans": "menu.tags",
            "icon": "icon ion-folder",
            "list": [{
                "type": "internal",
                "trans": "Dumb tag",
                "route": "public.taxonomies.slug({ term: 'post_tag', slug: 'dumb' })",
                "icon": "icon ion-iphone"
            }, {
                "type": "internal",
                "trans": "All",
                "route": "public.taxonomies({ term: 'post_tag' })",
                "icon": "icon ion-pricetags"
            }]
        },{
            "type": "internal",
            "trans": "menu.bookmarks",
            "route": "public.bookmarks",
            "icon": "icon ion-bookmark"
        }, {
            "type": "separator",
            "trans": "Separator"
        }, {
            "type": "external",
            "trans": "My blog",
            "href": "http://julienrenaux.fr",
            "icon": "icon ion-link"
        }, {
            "type": "internal",
            "trans": "params.title",
            "route": "public.params",
            "icon": "icon ion-gear-b"
        }, {
            "type": "internal",
            "trans": "about.title",
            "route": "public.about",
            "icon": "icon ion-information"
        }]
    },
    "settings": {
        "about": {
            "credit": true
        },
        "parameters": {
            "defaultFontSize": "medium"
        }
    },
    "cache": {
        "img": {
            "localCacheFolder": "imgcache",
            "useDataURI": false,
            "chromeQuota": 10485760,
            "usePersistentCache": true,
            "cacheClearSize": 0,
            "headers": {},
            "skipURIencoding": false
        },
        "data": {
            "capacity": 100,
            "maxAge": 21600000,
            "deleteOnExpire": "aggressive",
            "recycleFreq": 1000,
            "cacheFlushInterval": null,
            "storageMode": "localStorage"
        }
    },
    "bookmark": {
        "cache": {
            "capacity": 10
        }
    },
    "post": {
        "comments": {
            "enabled": true,
            "depth": 2,
            "per_page": 50
        },
        "cache": {
            "maxAge": 172800000,
            "capacity": 10
        }
    },
    "syntaxHighlighter": {
        "classPrefix": "hljs-",
        "tabReplace": "    ",
        "useBR": false,
        "languages": ["javascript", "html", "coffeescript", "html", "css", "scss", "json", "apache", "bash", "markdown", "less", "php", "apache", "typescript"]
    },
    "taxonomies": {
        "query": {
            "per_page": 5,
            "orderby": "count",
            "order": "desc"
        },
        "cache": {
            "maxAge": 172800000
        }
    },
    "authors": {
        "query": {
            "per_page": 5
        },
        "cache": {
            "capacity": 10,
            "maxAge": 86400000
        }
    },
    "pages": {
        "query": {
            "per_page": 5,
            "filter[orderby]": "date",
            "filter[order]": "desc"
        },
        "cache": {
            "capacity": 10,
            "maxAge": 86400000
        }
    },
    "search": {
        "query": {
            "per_page": 5,
            "filter[orderby]": "date",
            "filter[order]": "desc",
            "filter[post_status]": "publish"
        },
        "cache": {
            "capacity": 25,
            "maxAge": 86400000
        }
    },
    "posts": {
        "query": {
            "per_page": 5,
            "filter[orderby]": "date",
            "filter[order]": "desc",
            "filter[post_status]": "publish"
        },
        "cache": {
            "capacity": 25,
            "maxAge": 86400000
        }
    },
    "translation": {
        "displayed" : ["en", "fr", "zh", "es", "pl", "de"],
        "available": {
            "en": {
                "en_*": "en"
            },
            "fr": {
                "fr_*": "fr"
            },
            "zh": {
                "zh_*": "zh"
            },
            "es": {
                "es_*": "es"
            },
            "pl": {
                "pl_*": "pl"
            },
            "de": {
                "de_*": "de"
            }
        },
        "prefered": "en"
    },
    "analytics": {
        "enabled": false,
        "trackingId": "",
        "userId": "",
        "virtualPageTracking": true
    },
    "cordova": {
        "statubar": {
            "show": false,
            "color": "#455a64"
        },
        "appRate": {
            "enabled": false,
            "language": "en",
            "appName": "Your Blog title",
            "openStoreInApp": true,
            "usesUntilPrompt": 3,
            "promptForNewVersion": true,
            "useCustomRateDialog": "",
            "iosURL": "",
            "androidURL": "",
            "windowsURL": ""
        },
        "pushNotifications": {
            "enabled": false,
            "baseUrl": "http://yourDomain.com/pnfw",
            "android": {
                "senderID": ""
            },
            "ios": {
                "badge": true,
                "sound": true,
                "alert": true
            }
        }
    }
}
